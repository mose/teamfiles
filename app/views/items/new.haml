#uploader
:javascript
  $(function(){
    $("#uploader").pluploadQueue({
      runtimes: 'html5,browserplus,gears,flash,silverlight',
      url: '#{items_path}',
      multipart_params: {
        '#{request_forgery_protection_token}': '#{form_authenticity_token}',
        '#{Rails.application.config.session_options[:key]}': '#{request.session_options[:id]}'
      }
    });
  });
